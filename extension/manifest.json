{
  "manifest_version": 2,
  "name": "Get AH token",
  "description": "Get AH token",
  "version": "1.0",
  "permissions": ["cookies", "https://www.ah.nl/*", "http://localhost:3000/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://gogetmeals.herokuapp.com/*",
        "http://localhost:3000/*"
      ],
      "js": ["load.js"]
    }
  ]
}
