{
  "name": "shopping-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- jest --watch --no-coverage",
    "start": "node app.js",
    "watch": "nodemon app.js",
    "start:mock": "AH_API=ah.stub USE_TEST_DB=true USE_GOOGLE_AUTH_STUB=true node app.js",
    "test:api": "npm run start:mock & newman run api-tests/ShoppingAPItests.postman_collection.json -e api-tests/shopping.postman_environment.json",
    "test:newman": "newman run api-tests/ShoppingAPItests.postman_collection.json -e api-tests/shopping.postman_environment.json"
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "@google-cloud/translate": "^7.2.2",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "fs": "0.0.1-security",
    "gzip": "^0.1.0",
    "material-table": "^2.0.5",
    "mongodb": "^5.8.0",
    "morgan": "~1.10.0",
    "node-gzip": "^1.1.2",
    "paprika-api": "^1.0.3",
    "picnic-api": "^3.0.0",
    "ramda": "^0.29.0",
    "recipe-scraper": "^2.1.1",
    "request-promise": "^4.2.6",
    "uuidv1": "^1.6.14"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "babel-jest": "^29.6.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "newman": "^5.3.2",
    "nodemon": "^3.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.6.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
